{
  "name": "lambdas",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"nyaaaa test\" && exit 1",
    "hello": "sls invoke local -f hello --path src/functions/hello/mock.json && exit 0",
    "todotest": "sls invoke local -f graphql --path src/functions/graphql/Todo/mock.json && exit 0",
    "start": "MIVDV_ENV=OFFLINE sls offline start --allowCache",
    "dev": "MIVDV_ENV=OFFLINE sls offline start",
    "deploy": "sls deploy",
    "remove": "sls remove",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate:schema": "ts-node src/functions/graphql/scripts/exportSchema.ts > ../common/graphql/core.graphql",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "copy:env": "cp .env.example .env",
    "package": "serverless package"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "apollo-server": "^3.6.3",
    "apollo-server-core": "^3.6.3",
    "apollo-server-lambda": "^3.6.3",
    "graphql": "^15.5.3",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "6.0.9",
    "graphql-shield": "^7.5.0",
    "type-graphql": "^1.1.1",
    "class-validator":"^0.13.1",
    "prisma": "3.13.0",
    "@prisma/client": "3.13.0",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.6",
    "apollo-client": "^2.6.10",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-dotenv-plugin": "^3.10.0",
    "@anatine/esbuild-decorators": "^0.2.18",
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "esbuild": "^0.14.38",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-plugin-copy": "^1.2.1",
    "esbuild-plugin-tsc": "^0.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "faker": "^5.5.3",
    "json-schema-to-ts": "^1.6.4",
    "prettier": "^2.4.1",
    "prompts": "^2.4.2",
    "serverless-offline": "^8.8.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "type-fest": "^2.12.1",
    "typescript": "^4.5.5",
    "webpack": "^5.52.1",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}